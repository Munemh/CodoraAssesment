{
  "name": "VilleVie",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "small": "react-native run-ios --simulator='iPhone 8'",
    "pod-install": "cd ios && pod install && cd ..",
    "pod-update": "cd ios && pod update && cd ..",
    "repo-update": "cd ios && pod install --repo-update && cd ..",
    "bundle-install": "bundle install",
    "sha-keys": "keytool -list -v -keystore ./android/App/debug.keystore -alias androiddebugkey -storepass android -keypass android",
    "reset-cache": "npm start -- --reset-cache",
    "build-clean": "cd android && ./gradlew clean && cd ..",
    "reinstall": "rm -rf node_modules && npm install",
    "release": "cd android && ./gradlew bundleRelease && cd ..",
    "test-release": "cd android && ./gradlew assembleRelease && cd ..",
    "remove-app": "adb uninstall com.alltrueistic",
    "test-release-app": "cd android && ./gradlew app:assembleRelease && cd ..",
    "android-permission-execute": "chmod 755 android/gradlew",
    "peer-legacy": "npm install --legacy-peer-deps",
    "peer-legacy-permanent": "npm config set legacy-peer-deps true",
    "peer-legacy-save": "npm install --save --legacy-peer-deps",
    "open-xcode": "xed -b ios",
    "postinstall": "node patch.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@react-navigation/bottom-tabs": "^6.5.16",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "formik": "^2.2.9",
    "fs": "^0.0.1-security",
    "metro-react-native-babel-preset": "^0.77.0",
    "patch-package": "^8.0.0",
    "react": "18.2.0",
    "react-native": "0.73.5",
    "react-native-calendars": "^1.1304.1",
    "react-native-confirmation-code-field": "^7.3.2",
    "react-native-dimension": "^1.0.6",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-fast-image": "^8.6.3",
    "react-native-fs": "^2.20.0",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-normalize": "^1.0.1",
    "react-native-pie-chart": "^3.0.2",
    "react-native-reanimated": "^3.7.2",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^4.5.2",
    "react-native-screens": "^3.20.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.9.0",
    "react-native-svg-transformer": "^1.3.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "prettier": "2.8.8",
    "react-native-dotenv": "^3.4.11",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "engines": {
    "node": ">=18"
  }
}
